{
  "version": 2,
  "buildCommand": "cd client && pnpm install && pnpm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "functions": {
    "api/trpc.ts": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/trpc.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
